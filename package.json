{
  "name": "e621downloader",
  "version": "1.0.0-beta.4",
  "description": "Bulk download images from e621.",
  "productName": "e621downloader",
  "main": "build/src/index.js",
  "scripts": {
    "start": "tsc && electron-forge start -- --dev",
    "package": "electron-forge package",
    "make": "npm run make:linux && npm run make:win32",
    "make:win32": "electron-forge make --platform win32",
    "make:linux": "electron-forge make --platform linux",
    "publish": "dotenv electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Donovan_DMC",
    "email": "dwddaniels262@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FurryBotCo/E621Downloader.get"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/icons/global/icon",
        "platform": "all"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "E621Downloader",
            "exe": "E621Downloader.exe",
            "setupExe": "E621Downloader.Setup.exe",
            "setupMsi": "E621Downloader.Setup.msi",
            "title": "E621 Downloader",
            "iconUrl": "https://i.furry.cool/e621.ico",
            "setupIcon": "src/assets/icons/win/icon.ico",
            "icon": "src/assets/icons/win/icon.ico",
            "noMsi": false
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "categories": [
                "Utility"
              ],
              "genericName": "Image Downloader",
              "priority": "optional",
              "productDescription": "Bulk download images from e621.",
              "productname": "E621 Downloader",
              "section": "utils",
              "icon": "src/assets/icons/png/128x128.png",
              "productName": "e621downloader"
            }
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "name": "E621Downloader",
              "owner": "FurryBotCo"
            },
            "prerelease": true,
            "draft": true
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.1",
    "@types/js-yaml": "^3.12.5",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "dotenv": "^8.2.0",
    "electron": "10.1.2",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "rpmbuild": "0.0.23",
    "typescript": "^4.2.0-dev.20201109"
  },
  "dependencies": {
    "@octokit/core": "^3.2.1",
    "deasync": "^0.1.21",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.0",
    "morgan": "^1.10.0",
    "update-electron-app": "^2.0.1"
  }
}
